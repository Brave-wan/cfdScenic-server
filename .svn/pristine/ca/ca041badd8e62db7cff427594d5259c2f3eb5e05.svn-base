<%@ page contentType="text/html; charset=UTF-8"
	trimDirectiveWhitespaces="true"%>
<%@ include file="/WEB-INF/views-commons/header.jsp"%>
<div class="easyui-layout" data-options="fit:true">
	<div data-options="region:'north',border:false" style="height: 220px;">
		<div class="panel_title">
			<div style="float: left; padding: 10px">
				<i class="curLoca"></i><font class="fontbold">当前位置:</font>系统管理-分享规则设置
			</div>
		</div>
		<div class="titlediv">
			<div class="titleTxt">
				<h2>查询条件</h2>
				<em></em>
			</div>
		</div>
		<div class="querydiv">
			<p>
				<label for="searchValue">结算类别：</label> <select id="searchValue2"
					class="easyui-combobox"
					data-options="panelHeight: 'auto',editable:false">
					<option checked value="未结算">未结算</option>
					<option value="已结算">已结算</option>
				</select>
			</p>
			<p>
				<label for="searchValue">订单类别：</label> <select id="searchValue1"
					class="easyui-combobox"
					data-options="panelHeight: 'auto',editable:false">
					<option checked value="嗨旅行">嗨旅行</option>
					<option value="嗨服务">嗨服务</option>
				</select>
			</p>
			<p>
				<label for="searchValue">昵称：</label> 
				<input id="searchValue3" class="easyui-validatebox-text">
				
			</p>
			<p>
				<label for="searchValue">订单号：</label> 
				<input id="searchValue4" class="easyui-numberbox">
				
			</p>
			<p>
				<a href="javascript:void(0)" class="easyui-linkbutton"
					iconCls="icon-search" onclick="clearingManager.search()">查询</a>
			</p>
		</div>
		<div class="titlediv">
			<div class="titleTxt">
				<h2>分享规则设置</h2>
				<em></em>
			</div>
		</div>
		<div class="menubtndiv">
			<a id="shareRuleDelBtn" href="javascript:void(0)"
				onclick="clearingManager.clearing()" class="easyui-linkbutton"
				iconCls="icon-add">结算</a>
		</div>

	</div>
	<div data-options="region:'center',border:true">
		<div class="easyui-layout" data-options="fit:true">
			<div data-options="region:'center'">
				<table id="shareRuleGid" class="easyui-datagrid"
					data-options="
				url:'./background/Order/clearingData.json',
				border:false,
				fit:true,
				singleSelect:true,
				idField:'id',
				pagination:true,
				pageSize:20,
				striped:true,
				fitColumns:true">
					<thead>
						<tr>
							<th data-options="align:'center',width:100,field:'ID'">订单号</th>
							<th data-options="align:'center',width:100,field:'nickName'">昵称</th>
							<th data-options="align:'center',width:100,field:'type'">订单类型</th>
							<th data-options="align:'center',width:100,field:'createTime'">创建时间</th>
							<th data-options="align:'center',width:100,field:'orderAmt'">订单金额</th>
							<th data-options="align:'center',width:100,field:'status'">订单状态</th>
						</tr>
					</thead>
				</table>
			</div>
		</div>
	</div>
</div>
<input type="hidden" value="${b}" id="bTime"/>
<input type="hidden" value="${e}" id="eTime"/>
<script type="text/javascript"
	src="scripts/opensource/background/clearing/Manager.js"></script>
<%@ include file="/WEB-INF/views-commons/footer.jsp"%>
