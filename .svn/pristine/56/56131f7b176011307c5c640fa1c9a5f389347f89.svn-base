<%@ page contentType="text/html; charset=UTF-8"
	trimDirectiveWhitespaces="true"%>
<%@ include file="/WEB-INF/views-commons/header.jsp"%>
<div class="easyui-layout" data-options="fit:true">
	<div data-options="region:'north',border:false" style="height: 220px;">
		<div class="panel_title">
			<div style="float: left; padding: 10px">
				<i class="curLoca"></i><font class="fontbold">当前位置:</font>系统管理-提现申请审核
			</div>
		</div>
		<div class="titlediv">
			<div class="titleTxt">
				<h2>查询条件</h2>
				<em></em>
			</div>
		</div>
		<div class="querydiv">
			<p>
				<label for="searchValue">审核状态：</label> <select id="searchValue"
					name="type" class="easyui-combobox"
					data-options="panelHeight: 'auto',editable:false">
					<option checked value="1">申请中</option>
					<option value="2">申请通过</option>
				</select>
			</p>
			<p>
				<a href="javascript:void(0)" class="easyui-linkbutton"
					iconCls="icon-search" onclick="frtcenRechargelogManager.serch()">查询</a>
			</p>
		</div>
		<div class="titlediv">
			<div class="titleTxt">
				<h2>提现申请审核</h2>
				<em></em>
			</div>
		</div>
		<div class="menubtndiv">
			<a id="frtcenRechargelogYBtn" href="javascript:void(0)"
				onclick="frtcenRechargelogManager.toCheck(2)" class="easyui-linkbutton"
				iconCls="icon-redo">审核通过</a> <a id="frtcenRechargelogNBtn"
				href="javascript:void(0)" onclick="frtcenRechargelogManager.toCheck(0)"
				class="easyui-linkbutton" iconCls="icon-redo">审核失败</a>
		</div>

	</div>
	<div data-options="region:'center',border:true">
		<div class="easyui-layout" data-options="fit:true">
			<div data-options="region:'center'">
				<table id="frtcenRechargelogGid" class="easyui-datagrid"
					data-options="
				url:'./background/frtcenRechargeLog/frtcenRechargeLogData.json',
				border:false,
				fit:true,
				singleSelect:true,
				idField:'id',
				pagination:true,
				pageSize:20,
				striped:true,
				fitColumns:true">
					<thead>
						<tr>
							<th data-options="align:'center',width:100,hidden:true,field:'id'">id</th>
							<th data-options="align:'center',width:100,hidden:true,field:'userid'">userid</th>
							<th data-options="align:'center',width:100,hidden:true,field:'bankid'">bankid</th>
							<th data-options="align:'center',width:100,field:'nickname'">申请人</th>
							<th data-options="align:'center',width:100,field:'bankname'">提现银行</th>
							<th data-options="align:'center',width:100,field:'bankcard'">银行卡账号</th>
							<th data-options="align:'center',width:100,field:'virtualcoin'">蛙币</th>
							<th data-options="align:'center',width:100,field:'amount'">金额</th>
							<th data-options="align:'center',width:100,field:'rechargetime'">时间</th>
							<th data-options="align:'center',width:100,field:'status',formatter:function(v,r,i){
									switch (v) {
										case 0:
											return '申请失败';
										case 1:
											return '申请中';
										case 2:
											return '申请通过';
									}
								}">审核状态</th>
						</tr>
					</thead>
				</table>
			</div>
		</div>
	</div>
</div>
<script type="text/javascript"
	src="scripts/opensource/background/frtcenRechargeLog/Manager.js"></script>
<%@ include file="/WEB-INF/views-commons/footer.jsp"%>