	<select id="shopUserMessage" parameterType="Long" resultType="map">
		select si.id,
		si.name,
		si.shop_id,
		si.business_scope,
		si.account_type,
		si.account_name,
		si.bank_card,
		si.account_bank,
		si.is_license,
		si.license_img,
		si.other_img1,
		si.other_img2 
		from  shop_information si LEFT JOIN shop_user su on si.id=su.shop_information_id where su.id = #{id} and si.state=0 and si.is_audit=1
	</select>
	
	
	<select id="shopAutonymMessage" parameterType="Long" resultType="map">
		select si.id,
		si.realname,
		si.sex,
		si.idcard,
		si.hold_card_img,
		si.face_card_img,
		si.back_card_img 
		from shop_information si LEFT JOIN shop_user su on si.id=su.shop_information_id where su.id =  #{id} and si.state=0 and si.is_audit=1
	</select>

