<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.htkj.cfdScenic.app.dao.VisitorsProfilesDao">

	<select id="scenicSpotParticulars" resultType="map">
		select 
		id,
		name,
		html_url,
		DATE_FORMAT(create_time,'%Y-%c-%d %h:%i:%s') as create_time 
		from 
		visitors_profiles 
		where 
		visitors_type=1 and state=0 limit 1
	</select>
	<select id="getContentUrlById" parameterType="long" resultType="string">
		select
			html_url
		from 
			visitors_profiles
		where
			id = #{goodsId}
	</select>
	<select id="wetLandSynopsis" resultType="map" parameterType="Integer">
		select 
		id,
		name,
		html_url,
		DATE_FORMAT(create_time,'%Y-%c-%d %h:%i:%s') as create_time 
		from 
		visitors_profiles 
		where 
		visitors_type=#{type} and state=0 limit 1
	</select>
</mapper>