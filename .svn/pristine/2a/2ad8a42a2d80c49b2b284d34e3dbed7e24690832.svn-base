<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.htkj.cfdScenic.app.dao.ExpressDao">
	 <insert id="saveExpress" parameterType="com.htkj.cfdScenic.app.model.Express">
        INSERT INTO express
         <trim prefix="(" suffix=")" suffixOverrides=",">
             <if test="id != null">
                 id,
             </if>
             <if test="userId != null">
                 user_id,
             </if>
             <if test="orderCode != null">
                 order_code,
             </if>
             <if test="expressName != null">
                 express_name,
             </if>
             <if test="expressCode != null">
                 express_code,
             </if>
             <if test="createDate != null">
                 create_date,
             </if>
         </trim>
         <trim prefix="values (" suffix=")" suffixOverrides=",">
             <if test="id != null">
                 #{id},
             </if>
             <if test="userId != null">
                 #{userId},
             </if>
             <if test="orderCode != null">
                 #{orderCode},
             </if>
             <if test="expressName != null">
                 #{expressName},
             </if>
             <if test="expressCode != null">
                 #{expressCode},
             </if>
             <if test="createDate != null">
                 #{createDate},
             </if>
         </trim>
    </insert>
    <select id="getExpressInfo" parameterType="map" resultType="map">
    	select
    		express_name,
    		express_code,
    		create_date as expressCreateDate
    	from 
    		express
    	where 
    		user_id = ${userId} and order_code = ${orderCode}
    </select>
</mapper>