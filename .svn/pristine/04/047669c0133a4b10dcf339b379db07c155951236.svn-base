<%@ page contentType="text/html; charset=UTF-8"
	trimDirectiveWhitespaces="true"%>
<%@ include file="/WEB-INF/views-commons/header.jsp"%>
<style type="text/css">
body {
	font-family: "微软雅黑";
}

table {
	width: 90%;
	margin: 20px auto;
}

table td {
	height: 45px;
	text-align: left;
}

table td:first-child {
	width: 10%;
}

table td:last-child {
	width: 90%;
}

table td input[type=text] {
	width: 80%;
	height: 30px;
	line-height: 30px;
	padding-left: 10px;
}

table img {
	width: 150px;
	height: 100px;
}

table td textarea {
	padding-left: 10px;
}

table td label {
	float: left;
	width: 100px;
	text-align: left;
}

.l-btn-text {
	line-height: 30px;
}

.easyui-linkbutton {
	width: 30%;
	height: 30px;
	display: inline-block;
}
</style>
<div style="width: 98.5%; padding: 10px;">
	<div style="padding: 8px 2px; border-bottom: 1px solid #ccc">当前位置:用户申请审核-查看详情</div>
	<form id="ff" action="./background/UserInfo/editUserInfo"
		method="post">
		<input type="hidden" name="id" value="${m.id }" />
		<table>
			<tr>
				<td>昵称：</td>
				<td><input type="text" value="${m.nickname }" name="nickname"
					/></td>
			</tr>
			<tr>
				<td>手机号：</td>
				<td><input type="text" name="telphone" value="${m.telphone }"
					 /></td>
			</tr>
			<tr>
				<td>性别：</td>
				<td>
					<label><input type="radio" name="gender" <c:if test="${m.gender == 1 }">checked</c:if> value="1" />男</label>
					<label><input type="radio" <c:if test="${m.gender == 0 }">checked</c:if> name="gender" value="0" />女</label>
				</td>
			</tr>
			<tr>
				<td>生日：</td>
				<td><input type="text" value="${b }" name="birth"
					/></td>
			</tr>
			<tr>
				<td>年龄：</td>
				<td><input type="text" value="${m.age }" name="age"
					/></td>
			</tr>
			<tr>
				<td>身高：</td>
				<td><input type="text" value="${m.height }" name="height"
					/></td>
			</tr>
			<tr>
				<td>体重：</td>
				<td><input type="text" value="${m.weight }" name="weight"
					/></td>
			</tr>
			<tr>
				<td>星座：</td>
				<td><input type="text" value="${m.zodiac }" name="zodiac"
					/></td>
			</tr>
			<tr>
				<td>兴趣：</td>
				<td><input type="text" value="${m.interest }" name="interest"
					/></td>
			</tr>
			<tr>
				<td>蛙币：</td>
				<td><input type="text" value="${m.virtualcoin }" name="virtualcoin"
					/></td>
			</tr>
			<tr>
				<td>接单数：</td>
				<td><input type="text" value="${m.acceptscount }" name="acceptscount"
					/></td>
			</tr>
			<tr>
				<td>好评率：</td>
				<td><input type="text" value="${m.praise }" name="praise"
					/></td>
			</tr>
			<tr>
				<td>达人时薪：</td>
				<td><input type="text" value="${m.masterhourlyrate }" name="masterhourlyrate"
					/></td>
			</tr>
			<tr>
				<td>导游时薪：</td>
				<td><input type="text" value="${m.guideshourlyrate }" name="guideshourlyrate"
					/></td>
			</tr>
			<tr>
				<td>司机时薪：</td>
				<td><input type="text" value="${m.drivershourlyrate }" name="drivershourlyrate"
					/></td>
			</tr>
			<tr>
				<td>星级：</td>
				<td><input type="text" value="${m.starlevel }" name="starlevel"
					/></td>
			</tr>
			<tr>
				<td>头像：</td>
				<td><img src="${m.photoimage}" /></td>
			</tr>
		</table>
		<div style="text-align: center; padding: 5px">
			<a id="showPageSaveBtn" href="javascript:void(0)"
				onclick="$('#ff').submit()" class="easyui-linkbutton"
				iconCls="icon-save">确定</a> <a id="showPageCancleBtn"
				href="javascript:void(0)"
				onclick="window.location.href ='./background/UserInfo/toUserInfo'"
				class="easyui-linkbutton" iconCls="icon-cancel">取消</a>
		</div>
	</form>
</div>
<%@ include file="/WEB-INF/views-commons/footer.jsp"%>