package com.htrj.base.model;

import java.util.Date;

import com.htrj.common.base.BaseEntity;

public class BaseMenu extends BaseEntity {
	private static final long serialVersionUID = -4278724955150092948L;
	
	private Long 	bmId;				//id
	private Long 	bmParentId;			//上级id
	private Integer state;				//状态 0,open  1，closed是否有下级0：没有，1：有
	private Integer bmlevel;        	//层次
	private String  bmName;				//名称
	private String  bmUrl;				//路径
	private String  bmIcon;				//图标	
	private String  bmRemark;			//描述
	private Integer isDisplay;			//是否显示
	private String 	optId;				//操作人id
	private String  optName;			//操作人姓名
	private Date    currTime;			//操作时间
	private Integer sort;				//排序
	private Integer type;				//区分前后台功能
	
	/** *以下字段不是数据库中的字段，扩展 */
	private Long	brId;				//角色ID
	
	
	public Long getBmId() {
		return bmId;
	}
	public void setBmId(Long bmId) {
		this.bmId = bmId;
	}
	public Long getBmParentId() {
		return bmParentId;
	}
	public void setBmParentId(Long bmParentId) {
		this.bmParentId = bmParentId;
	}
	public Integer getState() {
		return state;
	}
	public void setState(Integer state) {
		this.state = state;
	}
	public Integer getBmlevel() {
		return bmlevel;
	}
	public void setBmlevel(Integer bmlevel) {
		this.bmlevel = bmlevel;
	}
	public String getBmName() {
		return bmName;
	}
	public void setBmName(String bmName) {
		this.bmName = bmName;
	}
	public String getBmUrl() {
		return bmUrl;
	}
	public void setBmUrl(String bmUrl) {
		this.bmUrl = bmUrl;
	}
	public String getBmIcon() {
		return bmIcon;
	}
	public void setBmIcon(String bmIcon) {
		this.bmIcon = bmIcon;
	}
	public String getBmRemark() {
		return bmRemark;
	}
	public void setBmRemark(String bmRemark) {
		this.bmRemark = bmRemark;
	}
	public Integer getIsDisplay() {
		return isDisplay;
	}
	public void setIsDisplay(Integer isDisplay) {
		this.isDisplay = isDisplay;
	}
	public String getOptId() {
		return optId;
	}
	public void setOptId(String optId) {
		this.optId = optId;
	}
	public String getOptName() {
		return optName;
	}
	public void setOptName(String optName) {
		this.optName = optName;
	}
	public Date getCurrTime() {
		return currTime;
	}
	public void setCurrTime(Date currTime) {
		this.currTime = currTime;
	}
	public Integer getSort() {
		return sort;
	}
	public void setSort(Integer sort) {
		this.sort = sort;
	}
	public static long getSerialversionuid() {
		return serialVersionUID;
	}
	public Long getBrId() {
		return brId;
	}
	public void setBrId(Long brId) {
		this.brId = brId;
	}
	public BaseMenu(){}
	public Integer getType() {
		return type;
	}
	public void setType(Integer type) {
		this.type = type;
	}
	

}
