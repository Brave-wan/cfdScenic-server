var frtcenRechargelogManager = {
	checkUrl : "./background/frtcenRechargeLog/checkFrtcenRechargeLog",
	init : function(){
		$(window).load(function(){
		});
	},	
	toCheck : function(t){
		var row=$('#frtcenRechargelogGid').datagrid('getSelected');
		if(!row){
			eu.showMsg("请选择一行再进行审核");
			return;
		}
		var m = "审核失败";
		if(t == 2){
			m = "审核通过";
		}
		$.messager.confirm("提示信息","您确定要"+m+"所选数据吗？",function(r){
			if(r){
				$.post(frtcenRechargelogManager.checkUrl,{id : row.id , status:t })
				.success(function(data){
								if(data.success){
									eu.showMsg("操作成功！");
									$("#frtcenRechargelogGid").datagrid('reload');
								}else{
									eu.showMsg(data);
								}
							}
						).error(function(data){
				        	eu.showMsg("系统错误，请联系管理员！");
				        })
			}else{
				$('#frtcenRechargelogGid').datagrid('clearSelections');
			}
			});
	},
	serch : function () {
 		var type = $('#searchValue').combobox('getValue');
		$('#frtcenRechargelogGid').datagrid('clearSelections');
		$('#frtcenRechargelogGid').datagrid('load',{status: type});
	}
};
frtcenRechargelogManager.init();