<%@ page contentType="text/html; charset=UTF-8"
	trimDirectiveWhitespaces="true"%>
<%@ include file="/WEB-INF/views-commons/header.jsp"%>
<div class="easyui-layout" data-options="fit:true">
	<div data-options="region:'north',border:false" style="height: 220px;">
		<div class="panel_title">
			<div style="float: left; padding: 10px">
				<i class="curLoca"></i><font class="fontbold">当前位置:</font>用户管理-银行卡管理
			</div>
		</div>
 	<div class="titlediv">
			<div class="titleTxt">
				<h2>查询条件</h2>
				<em></em>
			</div>
		</div>

		<div class="querydiv">
			<p>
				<label for="searchValueName">昵称：</label> 
				<input id="searchValueName" class="easyui-validatebox">
				
			</p>
			<p>
				<label for="searchValueBank">银行卡号：</label>
				<input id="searchValueBank" class="easyui-validatebox">
			</p>
			<p>
				<a href="javascript:void(0)" class="easyui-linkbutton"
					iconCls="icon-search" onclick="userBankManager.serch()">查询</a>
			</p>
		</div>

		<div class="titlediv">
			<div class="titleTxt">
				<h2>用户银行卡</h2>
				<em></em>
			</div>
		</div>
		<div class="menubtndiv">
		<!--
 			<a id="shareRuleAddBtn" href="javascript:void(0)" 
				onclick="shareRuleManager.add()" class="easyui-linkbutton"
				iconCls="icon-add">新增</a> <a id="shareRuleEditBtn"
				href="javascript:void(0)" onclick="shareRuleManager.edit()"
				class="easyui-linkbutton" iconCls="icon-redo">修改</a>
		-->
				<a id="shareRuleDelBtn" href="javascript:void(0)"
				onclick="userBankManager.del()" class="easyui-linkbutton"
				iconCls="icon-add">删除</a>
		</div>


	</div>
	<div data-options="region:'center',border:true">
		<div class="easyui-layout" data-options="fit:true">
			<div data-options="region:'center'">
				<table id="userBank" class="easyui-datagrid"
					data-options="
				url:'./background/UserInfo/getBankInfo.json',
				border:false,
				fit:true,
				singleSelect:true,
				idField:'id',
				pagination:true,
				pageSize:20,
				striped:true,
				fitColumns:true">
					<thead>
						<tr>
							<th
								data-options="align:'center',width:100,field:'userId',hidden:true">用户id</th>
							<th
								data-options="align:'center',width:100,field:'nickName'">用户昵称</th>
							<th
								data-options="align:'center',width:100,field:'userBankId',hidden:true">银行卡id</th>
							<th
								data-options="align:'center',width:100,field:'BANKNAME'">银行名称</th>
							<th
								data-options="align:'center',width:100,field:'BANKCARD'">卡号</th>
						</tr>
					</thead>
					</table>
				</div>
			</div>
		</div>
	</div>
</div> 

<script type="text/javascript"
	src="scripts/opensource/background/toUserBank/Manager.js"></script>
<%@ include file="/WEB-INF/views-commons/footer.jsp"%>
